entrypoint: ~()->(iErrorCode:~i64{0})
{
	bState:~i1{true};					// test true
	if(bState)
	{
		iErrorCode += i64{bState};
	}

	bState = false;						// test false
	if(!bState)							// test !
	{
		iErrorCode += i64{!bState};
	}

	if(i1{1}-bState)					// test -
	{
		iErrorCode += i64{i1{1}-bState};
		//iErrorCode += i64{-bState};
	}
	
	if(true-bState)					// test -
	{
		iErrorCode += i64{true-bState};
		//iErrorCode += i64{-bState};
	}

	bState = i1{1};						// test 1
	if(!!bState)						// test !!
	{
		iErrorCode += i64{!!bState};
	}

	if(!i1{2})							// test 2
	{
		iErrorCode += i64{!i1{2}};
	}
	if(i1{3})							// test 3 mod2 == 1
	{
		iErrorCode += i64{i1{3}};
	}

	bState = i1{0};						// test 0
	if(i1{1}+bState)					// test +
	{
		iErrorCode += i64{i1{1}+bState};
	}
	
	if(true+bState)
	{
		iErrorCode += i64{true+bState};
	}

	if(!bState && true)					// test &&
	{
		iErrorCode += i64{!bState && true};
	}

	if(!bState || false)				// test ||
	{
		iErrorCode += i64{!bState || false};
	}
}
