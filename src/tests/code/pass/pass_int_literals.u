entrypoint:~()->(i64{	0xAB +		// 171		Heyadecimal numbers starting with 0x
						0x0CD + 	// 205
						0x00EF +	// 239
						0q631 +		// 409		Octal numbers starting with 0q
						0b1011101 +	// 93		Binary numbers starting with 0b
						0b00100011 +// 35
						00064 +		// 64		Allow integers starting with zero
						2_048 +		// 2048		Allow inserting underscores
						0b_10_0000	// 32
						// 171+205+239+409+93+35+64+2048+32 = 3296
					}
				)